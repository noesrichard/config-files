// Coloque sus atajos de teclado en este archivo para sobreescribir los valores predeterminados
[
    //Crear archivo y carpeta cuando focus en el explorer
    {
        "key": "a",
        "command": "explorer.newFile",
        "when": "filesExplorerFocus"
    },
    {
        "key": "shift+a",
        "command": "explorer.newFolder",
        "when": "filesExplorerFocus"
    },

    //Toggle explorer
    {
        "key": "ctrl+n",
        "command": "workbench.view.explorer"
    },
    {
        "key": "ctrl+n",
        "when": "sideBarVisible",
        "command": "workbench.action.closeSidebar"
    },

    //Navegacion entre paneles
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusPreviousGroup"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusNextGroup"
    },
    //Cuando hay focus en el explorer se navega al primero o al ultimo
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusFirstEditorGroup",
        "when": "sideBarFocus"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusLastEditorGroup",
        "when": "sideBarFocus"
    },

    //Open vertical split
    {
        "key": "s",
        "command": "explorer.openToSide",
        "when": "filesExplorerFocus"
    }
]
